<launch>
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node" ns="zumo" required="true">
    <param name="port" value="tcp"/>
    <param name="tcp_port" value="11411"/>
    
    <remap from="rmotor_cmd" to="/zumo/differential_drive/rmotor_cmd"/>
    <remap from="lmotor_cmd" to="/zumo/differential_drive/lmotor_cmd"/>
  </node>
</launch>
