<!-- From example: https://github.com/pirobot/rbx1 -->
<launch>
    <!-- Run the map server with a blank map -->
    <node name="map_server" pkg="map_server" type="map_server" args="/move-base-configs/maps/blank_map.yaml" />

    <include file="/move-base-configs/blind/launch/fake_move_base.launch" />

    <!-- Run a static transform between /odom and /map -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="odom_map_broadcaster" args="0 0 0 0 0 0 1 map odom" />
    <node pkg="tf2_ros" type="static_transform_publisher" name="base_footprint_map_broadcaster" args="0 0 0 0 0 0 1 map base_footprint" />
    <node pkg="tf2_ros" type="static_transform_publisher" name="base_link_footprint" args="0 0 0 0 0 0 1 base_link base_footprint" />
</launch>